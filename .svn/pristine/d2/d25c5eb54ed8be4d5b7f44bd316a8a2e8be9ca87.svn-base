,KAAawF,eAA0BC,EAAvCzF,KAAuC0F,MAAAD,IAAAF,EAAwB,OAAAE,EAAA,OAAiBE,YAAA,iBAA2BF,EAAA,KAA3GzF,KAA2G8F,GAAA,eCE/I,IAcAmB,EAdyBH,EAAQ,OAcjCI,CACE9I,EACAgH,GATF,EAVA,SAAA+B,GACEL,EAAQ,SAaV,kBAEA,MAUeM,EAAA,QAAAH,EAAiB","file":"static/js/10.804f1892e168cd96d422.js","sourcesContent":["<template>\r\n  <div class=\"whole\">\r\n    <div class=\"kada_pc_picshow\" v-if=\"!isMobile\">\r\n      <div class=\"bg_header\"></div>\r\n      <div class=\"pic_bg\">\r\n        <div class=\"act-container\">\r\n          <div class=\"kada_title10\">\r\n            <!-- <img src=\"@/assets/img/title_pic.png\" alt=\"\" /> -->\r\n             <p>线下展览</p>\r\n          </div>\r\n          <div class=\"boder\">\r\n            <div class=\"picshow\" style=\"padding-top:70px\">\r\n            <ul class=\"clearfix\">\r\n              <li\r\n                v-for=\"(one, index) in pictureList\"\r\n                :key=\"index\"\r\n                @click=\"toDetail(one,index)\"\r\n              >\r\n                <div class=\"pic\">\r\n                  <img :src=\"one.cover\" alt=\"\" />\r\n                </div>\r\n                <div class=\"title\">\r\n                  <h3>{{ one.title |substr(12)}}</h3>\r\n                  <div class=\"status clearfix\">\r\n                    <span>执行承办地：{{one.address}}</span>\r\n                    <!-- <span>{{ one.ocVote }}赞</span>\r\n                    <img src=\"@/assets/imgwx/ic_zan_nor.png\" alt=\"\" @click.stop=\"userVote(one,1)\" v-if=\"one.opStatus == 1 && one.voteThisFlag == 0\" />\r\n                    <img src=\"@/assets/imgwx/ic_zan_sel.png\" alt=\"\" @click.stop=\"userVote(one,1)\" v-if=\"one.opStatus == 1 && one.voteThisFlag == 1\" /> -->\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            </ul>\r\n            <!-- <div class=\"kada_more\">查看更多</div> -->\r\n            <!-- <div class=\"pages\" style=\"padding-top: 51px\">\r\n              <pagination\r\n                :total=\"total\"\r\n                :page.sync=\"currentPage\"\r\n                :limit.sync=\"pageSize\"\r\n                @pagination=\"changePage\"\r\n              />\r\n            </div> -->\r\n          </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n       <navBar :url='url'></navBar>\r\n    </div>\r\n    <div class=\"kada_wx_picshow\" v-if=\"isMobile\">\r\n      <div class=\"headerbg\"></div>\r\n      <div class=\"kada_list\">\r\n      <div class=\"bor\">\r\n          <div class=\"boder\">\r\n          <div class=\"n_boder\">\r\n            <div class=\"title_top1\">\r\n              线下巡讲\r\n            </div>\r\n              <ul class=\"clearfix listt\">\r\n            <li\r\n              v-for=\"(one, index) in pictureList\"\r\n              :key=\"index\"\r\n              @click=\"toDetail(one,index)\"\r\n            >\r\n              <div class=\"pic\">\r\n                <img :src=\"one.cover\" alt=\"\" />\r\n              </div>\r\n              <div class=\"tit\">\r\n                <h3>{{ one.address | substr(8)}}</h3>\r\n              </div>\r\n            </li>\r\n          </ul>\r\n          </div>\r\n        </div>\r\n       </div>\r\n\r\n      </div>\r\n      <!-- backhome -->\r\n      <div class=\"back_home\" @click=\"back('/')\">\r\n        <img src=\"@/assets/imgwx/backlogo.png\" alt=\"\">\r\n      </div>\r\n      <!-- 分享 返回顶部 -->\r\n      <!-- 悬浮按钮 -->\r\n      <div class=\"top-share\">\r\n        <div class=\"listleft\" @click=\"shareAlert()\">\r\n          <img src=\"@/assets/imgwx/wx_share.png\" alt />\r\n        </div>\r\n        <div class=\"listtwo\" @click=\"toTo()\">\r\n          <img src=\"@/assets/imgwx/backtop.png\" alt />\r\n          <!-- <span>返回<br>顶部</span> -->\r\n        </div>\r\n      </div>\r\n      <!--微信分享指示-->\r\n      <div\r\n        class=\"hom-sharewraper\"\r\n        id=\"hom-sharewraper\"\r\n        style=\"display: none\"\r\n        @click=\"closedAlert()\"\r\n      >\r\n        <img src=\"@/assets/imgwx/share-alert.png\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Data from '@/data/staticData'\r\nimport QRCode from 'qrcodejs2'\r\nimport navBar from './components/navigatonBar'\r\nimport Pagination from '@/components/Pagination'\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n  components: {\r\n    Pagination,\r\n    navBar\r\n  },\r\n  computed: {\r\n    ...mapGetters(['isMobile', 'userInfo'])\r\n  },\r\n  data() {\r\n    return {\r\n      qrcodeFlag: false,\r\n      ocName: '',\r\n      total: 0,\r\n      pageSize: 16,\r\n      currentPage: 1,\r\n      pictureList: [],\r\n      searchSort: 0,\r\n      finished: false,\r\n      loading: false,\r\n      url: window.location.href\r\n    }\r\n  },\r\n  methods: {\r\n    userVote(item, num) {\r\n      if (item.flag !== 0) {\r\n        if (num === 1) {\r\n          this.$message.warning('今天以点赞，明天再来吧')\r\n        } else {\r\n          this.$toast('今天以点赞，明天再来吧')\r\n        }\r\n        return false\r\n      }\r\n      if (!this.userInfo) {\r\n        if (num === 1) {\r\n          this.$confirm('点赞需要登录，立即去登录', '提示', {\r\n            confirmButtonText: '确定',\r\n            cancelButtonText: '取消',\r\n            type: 'warning'\r\n          }).then(() => {\r\n            /*\r\n            参数说明：\r\n            userType：PCWOPERATE / WXOPERATE\r\n            preUrl： 当前页面地址,,注册页面地址\r\n            optype：TOUGAO / TOUPIAO\r\n          */\r\n            window.location.href =\r\n              window.location.origin +\r\n              '/users/getAuthorizeCode.login?preurl=' +\r\n              window.location.href +\r\n              ',,' +\r\n              window.location.origin +\r\n              '/21dxj/pick' + this.work +\r\n              '&userType=PCWOPERATE&optype=TOUGAO'\r\n            // this.$router.push({\r\n            //   path: '/articlecontribute'\r\n            // })\r\n          })\r\n        } else {\r\n          this.$dialog\r\n            .confirm({\r\n              title: '提示信息',\r\n              message: '您还未登录，请先登录',\r\n              confirmButtonText: '登录'\r\n            })\r\n            .then(() => {\r\n              window.location.href =\r\n                window.location.origin +\r\n                '/users/getAuthorizeCode.login?preurl=' +\r\n                window.location.href +\r\n                ',,' +\r\n                window.location.origin +\r\n                '/21dxj/pick' + this.work +\r\n                '&userType=WXOPERATE&optype=TOUPIAO'\r\n            })\r\n            .catch(() => {\r\n              // on cancel\r\n            })\r\n        }\r\n      } else {\r\n        this.vote(item)\r\n      }\r\n    },\r\n    vote(item) {\r\n      this.insertRead()\r\n      const params = {}\r\n      const _this = this\r\n      // 防止重复提交\r\n      if (!this.flag) {\r\n        this.flag = true\r\n        params.ocCode = item.ocCode\r\n        params.opClientType = 'CLIENT_WX'\r\n        params.operateCode = item.operateCode\r\n        this.$apis.userVote(params).then((res) => {\r\n          if (res.data.status.returnCode === 100) {\r\n            _this.flag = false\r\n            _this.$message.success('点赞成功')\r\n            item.ocVote++\r\n            item.voteThisFlag = 1\r\n            if (!res.data.data.list[0]) {\r\n              item.flag = 0\r\n            } else {\r\n              item.flag = 1\r\n            }\r\n          }\r\n        })\r\n      }\r\n    },\r\n    insertRead() {\r\n      // const _this = this\r\n      const params = {}\r\n      params.visitTitle = '浙江省红色主题雕塑大展'\r\n      this.$apis.insertVistor(params).then((res) => {\r\n      })\r\n    },\r\n\r\n    back() {\r\n      this.$router.push({ path: '/' })\r\n    },\r\n    goUp() {\r\n      let top = document.documentElement.scrollTop || document.body.scrollTop\r\n      // 实现滚动效果\r\n      const timeTop = setInterval(() => {\r\n        document.body.scrollTop = document.documentElement.scrollTop = top -= 50\r\n        if (top <= 0) {\r\n          clearInterval(t