  .back_home{
    position: fixed;
    right: 0;
    bottom: 6rem;
    width: 1.2rem;
    height: 1.2rem;
    img{
      width: 100%;
      height: 100%;
    }
  }
  .change_next{
    width: 1.5rem;
    height: 0.5rem;
    position: relative;
    left: 50%;
    font-size: 0.24rem;
    transform: translateX(-50%);
    border-radius: 0.2rem;
    margin-top: 0.3rem;
    line-height: 0.5rem;
    text-align: center;
    background-color: #F9ECC6;
  }
  .s-swiper .top {
    width: 100%;
    height: 1.78rem;
    background: linear-gradient(0deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 1) 100%);
    // opacity: 0.5;
    position: absolute;
    padding: 0 0.27rem;
    z-index: 9;
    display: flex;
    justify-content: space-between;
    padding-top: 0.36rem;
  }
  .s-swiper .top .page {
    width: 1.6rem;
    height: 0.66rem;
    border: 1px solid #ffffff;
    border-radius: 0.33rem;
    display: flex;
  }
  .s-swiper .top .page p {
    color: #ffffff;
    margin-left: 0.35rem;
    margin-top: 0.19rem;
    font-size: 0.28rem;
  }
  .s-swiper .top .page .home,
  .s-swiper .top .page .share {
    width: 0.38rem;
    height: 0.34rem;
  }
  .s-swiper .top .page .home img,
  .s-swiper .top .page .share img {
    width: 100%;
    height: 100%;
    margin-left: 0.3rem;
    margin-top: 0.15rem;
  }
  .hom-sharewraper{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.8);
    z-index: 9999;
    padding-top: 0.6rem;
    padding-left: 2.3rem;
  }
  .hom-sharewraper img{
    display: block;
    width: 4.77rem;
    height: 3.68rem;
  }
  .editor-show {
    //   background: #000;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #000;
    .s-swiper {
      .video_open{
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        width: 100%;
        // height: 4.5rem;
        video{
          width: 100%;
          height:auto;
        }
      }
      .bg {
        width: 100%;
        height: 100vh;
        position: relative;
        background-repeat: no-repeat;
        background-position: 50%;
        background-size: contain;
      //   background-size: auto 100%;
  
        .below {
          position: absolute;
          display: flex;
          bottom: 0;
          width: 100%;
          height: 1.64rem;
          background: linear-gradient(
            0deg,
            rgba(0, 0, 0, 1) 0%,
            rgba(0, 0, 0, 0) 100%
          );
          .left{
            width: 70%;
            .title {
              font-size: 0.28rem;
              line-height: 0.44rem;
              color: #ffffff;
              margin-left: 0.27rem;
            }
            p {
              margin-top: 0.1rem;
              font-size: 0.22rem;
              color: #cccccc;
              line-height: 0.44rem;
              margin-left: 0.27rem;
            }
          }
          .right{
            width: 30%;
            text-align: center;
            img{
              width: 0.5rem;
              height: 0.5rem;
            }
          }
          // opacity: 0.5;
         
        }
